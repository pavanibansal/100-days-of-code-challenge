Q91: Remove all vowels from a string.

/*
Sample Test Cases:
Input 1:
education
Output 1:
dctn
*/
#include <stdio.h>

int main() {
    char str[100], result[100];
    int i = 0, j = 0;

    printf("Enter a string: ");
    scanf("%[^\n]", str);  
    while (str[i] != '\0') {
        char ch = str[i];

        
        if (ch >= 'A' && ch <= 'Z')
            ch = ch + 32;

        if (ch != 'a' && ch != 'e' && ch != 'i' && ch != 'o' && ch != 'u') 
        {
            result[j] = str[i];
            j++;
        }

        i++;
    }

    result[j] = '\0';  
    printf("%s\n", result);
    return 0;
}
Q92: Find the first repeating lowercase alphabet in a string.

/*
Sample Test Cases:
Input 1:
stress
Output 1:
s
*/

#include <stdio.h>

int main() 
{
    char str[100];
    int freq[26] = {0};  
    int i = 0;
    char result = '\0';

    printf("Enter a string: ");
    scanf("%[^\n]", str);  
    while (str[i] != '\0') 
    {
        char ch = str[i];
        if (ch >= 'a' && ch <= 'z') 
        {
            freq[ch - 'a']++;  

            if (freq[ch - 'a'] == 2) 
            {  
                result = ch;
                break;
            }
        }
        i++;
    }

    if (result != '\0')
        printf("%c\n", result);
    else
        printf("No repeating lowercase alphabet\n");

    return 0;
}

